/dts-v1/;

#include "meson-sm1-odroid-c4.dts"

/ {
	model = "Hardkernel ODROID-C4";
};

&ethmac {
	/delete-property/	resets;
	/delete-property/	reset-names;
};

&gpu_opp_table {
	opp-999999984 {
		opp-hz = /bits/ 64 <999999984>;
		opp-microvolt = <800000>;
	};
};

&vddcpu {
	regulator-max-microvolt = <1030000>;
};
