/dts-v1/;
/plugin/;

/ {
	fragment@0 {
		target = <&i2c3>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@1 {
		target = <&spi0>;

		__overlay__ {
			status = "okay";

			spidev: spidev@0 {
				status = "okay";
				compatible = "odroid,spi-dev";
				reg = <0>;
				spi-max-frequency = <100000000>;
			};
		};
	};

	fragment@2 {
		target = <&uart1>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@3 {
		target = <&pwm9>;

		__overlay__ {
			status = "okay";
		};
	};
};
